{
  "exclude": ["node_modules", "**/dist/**"],
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "utils/*": ["utils/*"]
    }
  }
}
