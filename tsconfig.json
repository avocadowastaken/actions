{
  "exclude": ["node_modules"],
  "ts-node": {
    "transpileOnly": true
  },
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "utils/*": ["utils/*"]
    }
  }
}
